{
  "version": 2,
  "builds": [
      {
      "src": "dist/main.js",
      "use": "@vercel/node"
      }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "outputDirectory": "dist"
}